{
  "manifest_version": 2,
  "name": "extension1",
  "version": "1.0.0",
  "icons":
	{
		"16": "img/chrome.png",
		"48": "img/chrome.png",
		"128": "img/chrome.png"
  },
  "content_scripts": 
	[
		{
			"matches": ["<all_urls>"],
			"js": ["js/content-script.js"],
			"css": ["css/custom.css"],
			"run_at": "document_start"
		}
  ],
  "background":
	{
		"page": "html/background.html"
  },
  "browser_action": 
	{
		"default_icon": "img/chrome.png",
		"default_title": "这是一个示例Chrome插件",
		"default_popup": "html/popup.html"
  },
  "homepage_url": "https://www.baidu.com"
}